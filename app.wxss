/**app.wxss**/
.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;

} 

.complete-btn{
  width: 100%;
  text-align: center;
  height: 80rpx;
  background-color: #2E7D32;
  position: fixed;
  bottom: 0;
  
}

.complete-btn text{
  color: white;
  line-height: 80rpx;
}

.xbtn{
  padding: 10rpx 10rpx;
  height: 45rpx;
  text-align: center;
  border: 0.1rpx solid #ccc;
  font-size: 25rpx;
  line-height: 45rpx;
}

.mbtn{
  width: 150rpx;
  background-color: #2E7D32;
  color: white;
  text-align: center;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 20rpx;
}

text{
  font-size: 25rpx;
}
